@model BMOS.Models.FeedbackInfo

@{
    Layout = "_ManagePage";
    ViewData["Title"] = "Details";
}
}

<h1>Thông tin chi tiết</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Mã đánh giá
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FeedbackId)
        </dd>
        <dt class = "col-sm-2">
            Tên sản phẩm
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class = "col-sm-2">
            Thông tin đánh giá
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Content)
        </dd>
        <dt class = "col-sm-2">
            Số sao
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Star)/5
        </dd>
        <dt class = "col-sm-2">
            Thời gian
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.date)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index">Back to List</a>
</div>
